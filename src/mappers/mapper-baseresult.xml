<?xml version="1.0" encoding="UTF-8" ?> <!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="baseresult">
		<insert parameterType="java.util.HashMap" id="addDataUsingMap"> insert into
			baseresult values(#{serial}, #{player}, #{tried}, #{elapsed},
			sysdate) </insert>

		<insert parameterType="java.lang.String" id="addTrash"> insert into
			baseresult values(#{serial}, #{player}, 3, 2.2112, sysdate) </insert>

		<select id="getAllDataUsingMap" resultType="java.util.HashMap"> select * from
			baseresult order by tried asc, elapsed asc </select>

		<select id="getPlayer" resultType="java.lang.String"> select distinct player from
			baseresult order by player asc </select>

		<select parameterType="java.lang.String"
			id="getOneDataUsingMap" resultType="java.util.HashMap"> select * from baseresult where
			serial=#{serial } </select>

	</mapper>